
#-------------------------------------------------------------------------------
# ループ処理
#-------------------------------------------------------------------------------
if(exist("n")==0 || n<0) n = n0  # ループ変数の初期化

#-------------------------------------------------------------------------------
# プロット
#-------------------------------------------------------------------------------
set parametric
set key left top
plot "24sample_5_0.88.dat" every ::n*5 ::n*5 using 5:4 ps 5 pt 7 title "5","24sample_10_0.88.dat" every ::n*5 ::n*5 using 5:4 ps 5 pt 7 title "10", "24sample_25_0.88.dat" every ::n*5 ::n*5 using 5:4 ps 5 pt 7 title "20", "24sample_5_0.88.dat" every ::n*5 ::n*5 using 3:2 ps 5 pt 7 title "ex", [0:2*pi] cos(t),sin(t) lc 8
#-------------------------------------------------------------------------------
# ループ処理
#-------------------------------------------------------------------------------
n = n + dn            # ループ変数の増加
if ( n < n1 ) reread  # ループの評価
undefine n            # ループ変数の削除